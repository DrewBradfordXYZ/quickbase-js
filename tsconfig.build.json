{
  "extends": "./tsconfig.json",
  "compilerOptions": {
    "outDir": "dist",
    "rootDir": "src",
    "declaration": true,
    "allowImportingTsExtensions": false, // Override to false
    "noEmit": false // Emit .js and .d.ts
  },
  "include": ["src/**/*"],
  "exclude": ["node_modules", "dist", "src/generated-old"]
}
